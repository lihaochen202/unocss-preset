{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@lihaochen/unocss-preset": ["./packages/unocss-preset/src/index.ts"],
      "@lihaochen/unocss-preset-rem-to-vw": ["./packages/unocss-preset-rem-to-vw/src/index.ts"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
